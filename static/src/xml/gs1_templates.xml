<?xml version="1.0" encoding="utf-8"?>
<template xml:space="preserve">
    <t t-name="DateRange">
        <style>
            .o_main{
                padding-left: 0px !important;
            }
        </style>
        <div class="o_hr_attendance_kiosk_mode_container o_home_menu_background" style="flex-flow: row;">
            <div class="o_material_borrow_container ">
                <div class="o_borrow" style="overflow: auto;">
                    <br/><h2 class="mb-3">Filters</h2>
                    <table class="table table-sm" style="border-bottom: 1px black solid;">
                        <tbody>
                            <tr t-if="company.date_filters">
                                <th colspan="3" scope="row">Select Date</th>
                            </tr>
                            <tr t-if="company.date_filters">
                                <td colspan="3" style="background-color: lightgray;">
                                    <input type="date" t-attf-value="{{date}}" id="date_delivery_orders"/>
                                </td>
                            </tr>
                            <tr><td><br/></td></tr>
                            <tr t-if="company.filters">
                                <th colspan="3" scope="row">Group by</th>
                            </tr>
                            <tr t-if="company.filters">
                                <td t-if="company.customer_filters">
                                    <t t-if="filter === 'filter_user'">
                                        <button style="margin-top: 16px !important;" id="filter_user" class="red_button btn btn-primary mb16 filter_button">
                                            <i class="fa fa-user fa-2x"></i> Customer
                                        </button>
                                    </t>
                                    <t t-else="">
                                        <button style="margin-top: 16px !important;" id="filter_user" class=" btn btn-primary mb16 filter_button">
                                            <i class="fa fa-user fa-2x"></i> Customer
                                        </button>
                                    </t>
                                </td>
                                <td t-if="company.location_filters">
                                    <t t-if="filter === 'filter_loc'">
                                        <button style="margin-top: 16px !important;" id="filter_loc" class="red_button btn btn-primary mb16 filter_button">
                                            <i class="fa fa-map-marker fa-2x"></i> Location
                                        </button>
                                    </t>
                                    <t t-else="">
                                        <button style="margin-top: 16px !important;" id="filter_loc" class="btn btn-primary mb16 filter_button">
                                            <i class="fa fa-map-marker fa-2x"></i> Location
                                        </button>
                                    </t>
                                </td>
                                <td t-if="company.delivery_zone_filters">
                                    <t t-if="filter === 'filter_zone'">
                                        <button style="margin-top: 16px !important;" id="filter_zone" class="red_button btn btn-primary mb16 filter_button">
                                            <i class="fa fa-truck fa-2x"></i> Delivery zone
                                        </button>
                                    </t>
                                    <t t-else="company.delivery_zone_filters">
                                        <button style="margin-top: 16px !important;" id="filter_zone" class="btn btn-primary mb16 filter_button">
                                            <i class="fa fa-truck fa-2x"></i> Delivery zone
                                        </button>
                                    </t>
                                </td>
                            </tr>
                            <t t-if="filter === 'filter_zone' and company.delivery_zone_filters">
                                <tr  class="zone_selector">
                                    <td colspan="3">
                                        <select class="zone_select" name="select">
                                            <option value="0"></option>
                                                <t t-foreach="zones" t-as="z">
                                                    <option t-attf-value="{{z.id}}"><t t-esc="z.display_name"/></option>
                                                </t>
                                        </select>
                                    </td>
                                </tr>
                                <tr class="zone_selector">
                                    <td colspan="3" class="zones_selected">
                                        <t t-if="selected">
                                            <t t-foreach="selected" t-as="s">
                                                <span style="border-radius: 25px;font-size: 100%" t-attf-id="{{s.id}}" class="badge badge-success">
                                                    <t t-esc="s.display_name"/>
                                                    <button style="margin-left: 2%;" class="erase_badge">
                                                        <strong>x</strong>
                                                    </button>
                                                </span> 
                                            </t>
                                        </t>
                                    </td>
                                </tr>
                            </t>
                            <t t-else="company.delivery_zone_filters">
                                <tr  class="zone_selector no_display">
                                    <td colspan="3">
                                        <select class="zone_select" name="select">
                                            <option value="0"></option>
                                            <t t-foreach="zones" t-as="z">
                                                <option t-attf-value="{{z.id}}"><t t-esc="z.display_name"/></option>
                                            </t>
                                        </select>
                                    </td>
                                </tr>
                                <tr class="zone_selector no_display">
                                    <td colspan="3" class="zones_selected">
                                    </td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                    <div class="row btn-group buttons mt-4">
                        <button class="btn btn-danger m-1 back">
                            <h3 class="m-1">Go Back</h3>
                        </button>
                        <button class="btn btn-primary m-1 coninue_to_delivery_orders">
                            <h3 class="m-1">Continue</h3>
                        </button>
                    </div>  
                </div>     
            </div> 
        </div> 
    </t>
    <t t-name="WarehouseSelection">
        <style>
            .o_main{
                padding-left: 0px !important;
            }
        </style>
        <div class="o_home_menu_background">
            <div class="o_material_borrow_container">
                <div class="o_borrow">
                    <h2 class="mb-4">Warehouse Operations</h2>
                    <t t-foreach="warehouses" t-as="w">
                        <div class="row o_hr_attendance_kiosk_welcome_row">
                            <div class="col-sm-12 mb-3">
                                <button t-attf-id="{{w.id}}" class="btn btn-primary col-12 wh_op">
                                    <h3 class="m-2"><t t-esc="w.name"/></h3>
                                </button>
                            </div>
                        </div>
                    </t>
                </div>
            </div>
        </div>
    </t>
    <t t-name="PickingLines">
        <style>
            .o_main{
                padding-left: 0px !important;
            }
        </style>
        <div class="o_hr_attendance_kiosk_mode_container o_home_menu_background" style="flex-flow: row;">
            <div class="o_material_borrow_container ">
                <div class="o_borrow" style="overflow: auto;max-width: 650px;!important;">
                    <h2 class="mb-3"><t t-esc="filter"/></h2>
                    <h5 class="mb-3"><input type="checkbox" id="check_all"/> Check all</h5>
                    <div id="accordion" style="margin-top: 15px;">
                        <t t-foreach="pickings" t-as="p">
                            <div class="card">
                                <div class="card-header" style="background-color: aqua;">
                                    <h5 class="mb-0">
                                        <button t-if="p[0][0]" style="color: black;white-space: normal;" class="btn btn-link collapsed" data-toggle="collapse" t-attf-data-target="{{'#'+'id_'+p[0][0].id}}" aria-expanded="true" aria-controls="collapseOne">
                                            <t t-if="p[0][0] and p[0][0].name" t-esc="p[0][0].name"/>
                                            <t t-else="">Not defined</t>
                                        </button>
                                    </h5>
                                </div>
                                <div t-if="p[0][0]" t-attf-id="{{'id_'+p[0][0].id}}" class="collapse">
                                    <div class="card-body">
                                        <div id="accordion2" style="margin-top: 15px;">
                                            <t t-foreach="p[1]" t-as="p2">
                                                <div class="card">
                                                    <div class="card-header" style="background-color: beige;">
                                                        <div class="row">
                                                            <div class="col-2">
                                                                <input class="picking_ch" type="checkbox" t-attf-id="{{'pid_'+p2[0].id}}"/> 
                                                            </div>
                                                            <div class="col-10">
                                                                <h5 t-attf-id="{{'p_'+p2[0].id}}" class="picking"><t t-esc="p2[0].name"/></h5>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </t>
                                        </div>                        
                                    </div>
                                </div>
                            </div>    
                        </t>
                    </div>
                    <div class="row btn-group buttons mt-4">
                        <button class="btn btn-danger m-1 back_to_date">
                            <h3 class="m-1">Go Back</h3>
                        </button>
                        <button class="btn btn-primary m-1 coninue_to_delivery_orders">
                            <h3 class="m-1">Select</h3>
                        </button>
                    </div>  
                </div>
            </div>
        </div> 
    </t>

    <t t-name="ShowMoveLine">
        <style>
            .o_main{
                padding-left: 0px !important;
            }
        </style>
        <div class="o_home_menu_background">
            <div class="o_material_borrow_container">
                <div class="o_borrow" style="overflow: auto;padding: 0 !important"><br/>
                    <h2 class="mb-3"><t t-esc="line.picking_id[1]"/></h2>
                    <div id="wrap" class="oe_structure oe_empty">
                        <div class="card">
                            <div class="card-header">
                                <t t-esc="line.product_id[1]"/>
                            </div>
                            <div class="card-body">
                                <table class="table table-sm">
                                    <tbody>
                                        <tr t-if="line.product_tracking != 'none'">
                                            <th scope="row">Lot
                                                <t t-if="type == 'incoming'">
                                                    <a style="margin-left: 1%;margin-bottom:0px !important;" class="create_lot">
                                                        <i class="fa fa-pencil-square-o fa-1x"></i>
                                                    </a>
                                                </t>
                                            </th>
                                            <td t-if="line.current_lot" t-attf-id="{{line.current_lot.id}}" class="col-6 selected_lot" style="background-color: lightgray;"><t t-esc="line.current_lot.name"/></td>
                                        </tr>
                                        <tr t-if="line.owner_id">
                                            <th scope="row">Owner</th>
                                            <td class="col-6">
                                                <input type='text' t-att-value='line.owner_id' class='col-6 form-control' readonly="1"/>
                                            </td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Done</th>
                                            <td class="col-6">
                                                <t t-if="line.product_id.measure_type == 'unit'">
                                                    <input id="done_integer_input" t-att-value="done" type='number' min="0" t-att-max="line.product_uom_qty" class='col-6 form-control done'/>
                                                </t>
                                                <t t-if="line.product_id.measure_type != 'unit'">
                                                    <input id="done_float_input" t-att-value="done" type='number' min="0" t-att-max="line.product_uom_qty" class='col-6 form-control done'/>
                                                </t>
                                            </td>
                                        </tr>  
                                        <tr>
                                            <th scope="row">To do</th>
                                            <td class="col-6">
                                                <input type='number' t-att-value='line.product_uom_qty-line.qty_done' class='col-6 form-control to_do_total' readonly="1"/>
                                            </td>
                                        </tr>    
                                        <t t-if="line.product_tracking != 'none' and line.product_tracking != 'serial' and is_in_group and line.product_id.expiration_time != 0">
                                            <tr t-if="type == 'incoming'">
                                                <th scope="row">Correct State</th>
                                                <td class="col-6">
                                                    <!--<t t-log="line.correct_state_of_product"/>-->
                                                    <input type="checkbox" class="col-1 form-control correct_state_of_product" checked="1"/>
                                                </td>
                                            </tr>
                                            <tr class="bad_quality_description_div" t-if="type == 'incoming'">
                                                <th scope="row">Reason</th>
                                                <td class="col-6">
                                                    <input type='text' t-att-value='line.bad_quality_description' class='form-control bad_quality_description'/>
                                                </td>
                                            </tr> 
                                            <tr t-if="type == 'incoming'">
                                                <th scope="row">Quality Check</th>
                                                <td class="col-6">
                                                    <!--<t t-log="line.quality_check"/>-->
                                                    <t t-if="line.quality_check">
                                                        <input type="checkbox" class="col-1 form-control quality_check" checked="1" disabled="disabled"/>
                                                    </t>
                                                    <t t-else="">
                                                        <input type="checkbox" class="col-1 form-control quality_check" disabled="disabled"/>
                                                    </t>
                                                </td>
                                            </tr> 
                                            <tr t-if="type == 'incoming' and !line.quality_check">
                                                <td colspan="2">
                                                    <div class="col-12">
                                                        <i class="fa fa-exclamation-triangle fa-2x" style="color: crimson;"></i>
                                                        <t t-esc="line.reason_quality"/>
                                                    </div>  
                                                </td>
                                            </tr>    
                                        </t>                                
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="row btn-group buttons mt-4">
                            <button class="btn btn-danger m-1 back">
                                <h3 class="m-1">Go Back</h3>
                            </button>
                            <button class="btn btn-success m-1 validate_move_lines">
                                <h3 class="m-1">Validate</h3>
                            </button>
                        </div>  
                    </div>
                    <div class="modal" id="modal" tabindex="-1" role="dialog">
                        <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title">Divide operation</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">x</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <p>You will move fewer items. Do you want to move fewer items or move the remaining ones later?</p>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-primary divide">Divide</button>
                                    <!--<button type="button" class="btn btn-primary divide">Move fewer</button>-->
                                    <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button><br/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id ="myModal" class="modal" tabindex="-1" role="dialog">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title">Create lot</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true">x</span>
                                    </button>
                                </div>
                                <div class="modal-body">
                                    <form name="myForm">
                                        Name: <input class="lot_name" type="text" name="fname"/><br/>
                                        Exp√¨ration Date: <input type="date" id="expiration_date"/>
                                    </form> 
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="confirm_create_lot btn btn-primary">Create</button>
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>
     <t t-name="SelectBarcode">
        <style>
            .o_main{
                padding-left: 0px !important;
            }
        </style>
        <div class="o_home_menu_background">
            <div class="o_material_borrow_container">
                <div class="o_borrow">
                    <h2 class="mb-4">Select barcode</h2>
                    <div class="row o_hr_attendance_kiosk_welcome_row">
                        <div class="col-sm-12 mb-3">
